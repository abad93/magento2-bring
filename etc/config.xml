<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <bring>
                <active>1</active>
                <model>Markant\Bring\Model\Carrier</model>
                <name>Bring</name>
                <title>Bring</title>
                <type>I</type>
                <enabled_methods>SERVICEPAKKE,PA_DOREN</enabled_methods>
                <bring_client_url>http://example.com</bring_client_url>
                <default_product_weight>0.25</default_product_weight>
                <mybring_client_uid></mybring_client_uid>
                <mybring_api_key></mybring_api_key>
                <calculation>
                    <use_packagesize>0</use_packagesize>
                    <posting_at_post_office>0</posting_at_post_office>
                    <edi>1</edi>
                    <activate_api>1</activate_api>
                    <show_estimated_delivery>1</show_estimated_delivery>
                </calculation>
                <booking>
                    <activate_booking>0</activate_booking>
                    <test>1</test>
                    <default_customer></default_customer>
                    <package>
                        <width>31</width>
                        <length>21</length>
                        <height>6</height>
                    </package>
                    <origin>
                        <name></name>
                        <country_id></country_id>
                        <street_line_1></street_line_1>
                        <street_line_2></street_line_2>
                        <postcode></postcode>
                        <city></city>
                        <email></email>
                        <phone_number></phone_number>
                    </origin>
                </booking>
            </bring>
        </carriers>
    </default>
</config>